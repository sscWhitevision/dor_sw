<li class="contentblocks-video-preview-holder">
    <a href="https://www.youtube.com/watch?v={%=o.id.videoId%}" target="_blank" data-video_id="{%=o.id.videoId%}" class="contentblocks-video-preview">
        <img src="{%=o.snippet.thumbnails.medium.url%}" alt="{%=o.snippet.title%}">
        <h4>{%=o.snippet.title%}</h4>
        <p>By {%=o.snippet.channelTitle%} on {%=o.snippet.publishedAt%}</p>
    </a>
</li>
